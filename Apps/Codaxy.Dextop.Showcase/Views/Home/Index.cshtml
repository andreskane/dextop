@{
    ViewBag.Title = "Dextop Showcase";
    ViewBag.Layout = null;
}
@model Codaxy.Dextop.Showcase.Models.DextopAppConfig
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>            
    @foreach (var css in @Model.CssFiles)
    {
    <link href="@css" rel="stylesheet" type="text/css" />
    }
    @foreach (var js in @Model.JsFiles)
    {
    <script src="@js" type="text/javascript"></script>
    }   
</head>
<body>   
    <!-- Fields required for history management --> 
    <form id="history-form" class="x-hide-display" action="#"> 
        <input type="hidden" id="x-history-field" /> 
        <iframe id="x-history-frame"></iframe> 
    </form>

    <!-- To see source code go to https://github.com/codaxy/dextop -->
    <script type="text/javascript">       
        Ext.onReady(function() {            
            Dextop.Session.initialize(@Model.SessionConfig);
        });
    </script>
    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-20113409-3']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();
    </script>
</body>
</html>
